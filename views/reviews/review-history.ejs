  <% include ../partials/boilerplate %>
 
  <div class='container'>
     <div class='history'>
        <div class='navigation'>
        <a href="/review/search"> < Go Back to Search </a>
        <a href="/review/add">Add a New Rating +</a>
         </div>
     <% for (let review of data) { %>
         <div class='content'>
        <div class='title'>
        <h1><%= review.res_name %></h1>
        </div>        

        <div class='review'> 
            <h3>Rating : <%= review.rating %></h3>
            <h3>Memo : <%= review.memo %></h3>
        </div> 
        <div class='actionbar'>
         <form method="POST" action='/review/<%= review.id %>?_method=DELETE'>
                   <input type="submit" value="Delete it!"/> 
                </form>
         <form method="GET" action='/review/<%= review.id %>/edit'>
                   <input type="submit" value="Edit it!"/> 
                </form>
                </div>
        </div>
    <% } %>
 </div>
</div>
 </div>

  <% include ../partials/end %>