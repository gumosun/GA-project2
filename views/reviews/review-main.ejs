  <% include ../partials/boilerplate %>
 <div class='container'>
     <div class='mainback'>
     <% for (let review of data) { %>
        <div class='title'>
        <h1><%= review.res_name %></h1>
        </div>        
        <div class='myreview'>
            <h3>My Rating</h3>
        </div>
        <div class='review'> 
            <h3>Rating : <%= review.rating %></h3>
